<h1>Event List</h1>
<p>
  <%= link_to "Latest events", latest_events_path %>
</p>



<table>
<% @events.each do |event| %>
  <tr>
    <td>
      <%= event.name%>
    </td>
    <td>
      <%= link_to 'Show', event_path(event), :class => "btn btn-primary"  %>
    </td>
    <td>
      <%= link_to 'location', event_location_path(event), :class => "btn btn-success" %>
    </td>
    <td>
      <%= link_to "Attendee List", event_attendees_path(event), :class => "btn btn-info" %>
    </td>
    <td>
      <%= link_to " (XML)", event_path(event, :format => :xml), :class => "btn btn-default" %>
    </td>
    <td>
      <%= link_to " (JSON)", event_path(event, :format => :json), :class => "btn btn-default" %>
    </td>
    <td>
      <%= link_to 'Edit', edit_event_path(event), :class => "btn btn-warning"%>
    </td>
    <td>
      <%= link_to 'Delete', event_path(event), :method => :delete, :class => "btn btn-danger" %>
    </td>
  </tr>
<% end %>
</table>
<p>
<%= link_to 'New Event', new_event_path %>
</p>
<%= paginate @events %>